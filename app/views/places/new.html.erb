<h1>New place</h1>

<% form_for(@place) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :users_id %><br />
    <%= f.text_field :users_id %>
  </p>
  <p>
    <%= f.label :coordinates_lat %><br />
    <%= f.text_field :coordinates_lat %>
  </p>
  <p>
    <%= f.label :coordinates_long %><br />
    <%= f.text_field :coordinates_long %>
  </p>
  <p>
    <%= f.label :direction %><br />
    <%= f.text_field :direction %>
  </p>
  <p>
    <%= f.label :countries_id %><br />
    <%= f.text_field :countries_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :rank %><br />
    <%= f.text_field :rank %>
  </p>
  <p>
    <%= f.label :image_url %><br />
    <%= f.text_field :image_url %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', places_path %>