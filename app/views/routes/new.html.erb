<h1>New route</h1>

<div id="form_fields">
<% form_for(@route) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :rank %>
    <%= f.text_field :rank %>
  </p>
  <p>
    <%= f.label :users_id %>
    <%= f.text_field :users_id %>
  </p>
  <p>
    <%= f.label :places_id %>
    <%= f.text_field :places_id %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', routes_path %>

</div>